{
  "name": "lucky-js",
  "version": "1.0.0",
  "main": "./src/js/server-app.js",
  "scripts": {
    "build-js": "browserify -dt reactify ./src/js/browser-app.js -o ./public/javascripts/bundle.js -v",
    "build-css": "cat ./public/stylesheets/styles/*.css > ./public/stylesheets/bundle.css",
    "build": "npm run build-js && npm run build-css",
    "start": "foreman start",
    "test": "jasmine"
  },
  "dependencies": {
    "abstract-blob-store": "^3.2.0",
    "body-parser": "~1.12.0",
    "browserify": "^13.0.1",
    "catw": "^1.0.1",
    "cookie-parser": "~1.3.4",
    "cookie-session": "^1.1.0",
    "cors": "^2.7.1",
    "cron": "^1.1.0",
    "debug": "^2.1.3",
    "ejs": "^2.3.4",
    "express": "~4.12.2",
    "ipfs": "^0.14.1",
    "ipfs-api": "@2.4.1",
    "ipfs-block": "^0.3.0",
    "ipfs-block-service": "^0.4.0",
    "ipfs-log": "^1.4.9",
    "ipfs-repo": "^0.8.0",
    "mime-db": "^1.20.0",
    "morgan": "~1.5.1",
    "oboe": "^2.1.2",
    "react": "^15.2.1",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.2.1",
    "reactify": "^1.1.1",
    "request": "^2.72.0",
    "underscore": "^1.8.3",
    "xhr": "^2.2.1"
  },
  "engines": {
    "node": "5.1.0"
  },
  "jasmine": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "devDependencies": {
    "jasmine": "^2.3.1",
    "jsdom": "^3.1.2"
  },
  "license": "All Rights Reserved",
  "private": true
}
